<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Detormentor_CLIENT.ServerIPInput">
    <StackLayout VerticalOptions="Center">
        <Entry x:Name="IPstring" HorizontalOptions="CenterAndExpand" Placeholder="Server IP" WidthRequest="300"/>
        <Button x:Name="IPbutton" Text="Set IP" HorizontalOptions="CenterAndExpand" Clicked="submitIP"></Button>
    </StackLayout>
</ContentPage>